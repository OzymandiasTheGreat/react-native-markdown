{
	"name": "@oz/react-native-markdown",
	"version": "0.0.0",
	"description": "Render markdown to native/semantic components. Supports expo, GFM, inline HTML, and syntax highlighting with themes.",
	"main": "lib/commonjs/index.js",
	"module": "lib/module/index.js",
	"react-native": "src/index",
	"types": "lib/types",
	"author": {
		"name": "Tomas Ravinskas",
		"email": "tomas.rav@gmail.com",
		"url": "https://tomasrav.me/"
	},
	"license": "MIT",
	"scripts": {
		"build": "rm -rf lib && tsc --module es6 --outDir 'lib/module' && tsc --module commonjs --outDir 'lib/commonjs'",
		"release": "release-it --npm.yarn1"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"@expo/html-elements": "^0.1.0",
		"@jsamr/counter-style": "^2.0.2",
		"@jsamr/react-native-li": "^2.3.1",
		"@types/react-syntax-highlighter": "^13.5.2",
		"color-string": "^1.9.0",
		"react-native-render-html": "^6.3.4",
		"react-native-syntax-highlighter": "^2.1.0",
		"remark-gfm": "^3.0.1",
		"remark-parse": "^10.0.1",
		"string.prototype.matchall": "^4.0.6",
		"unified": "^10.1.1"
	},
	"peerDependencies": {
		"react": "*",
		"react-native": "*"
	},
	"devDependencies": {
		"@types/color-string": "^1.5.2",
		"@types/react": "17.0.39",
		"@types/react-native": "0.64.3",
		"@types/string.prototype.matchall": "^4.0.1",
		"release-it": "^14.12.5",
		"typescript": "^4.5.5"
	},
	"resolutions": {
		"react": "17.0.1"
	}
}
